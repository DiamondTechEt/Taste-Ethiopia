{
  "expo": {
    "name": "Taste of Ethiopia",
    "slug": "taste-of-ethiopia",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tasteofethiopia",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8cad474b-06d0-476f-af1f-6acaf52e3efd"
      }
    },
    "android": {
      "package": "com.leulak.tasteofethiopia"
    }
  }
}
